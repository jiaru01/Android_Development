<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.buypool">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/appName"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".NoticeActivity"></activity>
        <activity android:name=".CardSendActivity" />
        <activity android:name=".CardCollectionActivity" />
        <activity
            android:name=".ShowCardDetailActivity"
            android:parentActivityName=".BuyPoolDisplayPageActivity" />
        <activity android:name=".BuyPoolDisplayPageActivity" />
        <activity android:name=".ProfileActivity" />
        <activity android:name=".MenuActivity" />
        <activity android:name=".SignUpActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>